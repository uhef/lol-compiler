{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
      "arrow-parens": false,
      "interface-name": false,
      "max-classes-per-file": false,
      "object-literal-sort-keys": false,
      "ordered-imports": true,
      "trailing-comma": false,
      "no-console": false,
      "variable-name": false,
      "member-access": false,
      "eofline": false,
      "prettier": true
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
